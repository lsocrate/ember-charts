{
  "name": "ember-charts-shim",
  "version": "1.2.0",
  "homepage": "http://addepar.github.io/#/ember-charts",
  "authors": [
    "Addepar"
  ],
  "description": "A charting library built with the Ember.js and d3.js frameworks. It includes time series, bar, pie, and scatter charts which are easy to extend and modify. The out-of-the-box behavior these chart components represents our thoughts on best practices in chart interactivity and presentation.",
  "main": "dist/ember-charts.js",
  "moduleType": [
    "globals"
  ],
  "keywords": [
    "ember",
    "charts"
  ],
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests",
    "app",
    "src",
    "dependencies/addepar-bootstrap-theme",
    "dependencies/bootstrap",
    "dependencies/compiled",
    "dependencies/d3.v3.js",
    "dependencies/ember-charts.js",
    "dependencies/ember.js",
    "dependencies/font-awesome",
    "dependencies/google-code-prettify",
    "dependencies/handlebars.js",
    "dependencies/jquery-1.9.1.js",
    "dependencies/jquery-ui",
    "dependencies/lodash.js"
  ],
  "dependencies": {
    "bootstrap": "~3.1.1",
    "lodash": "~2.4.1",
    "jquery": "~1.9.1",
    "ember": "~1.5.1",
    "handlebars": "~1.3.0",
    "d3": "~3.2.7"
  }
}
